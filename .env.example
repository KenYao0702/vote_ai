# Google OAuth Credentials
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# The full URL to your backend's Google auth callback
REDIRECT_URI=http://localhost:3000/auth/google/callback

# The URL of the frontend, for auth redirects
FRONTEND_URL=http://localhost:8080

# --- Blockchain Settings ---

# The private key of the account that will fund new user wallets.
# This should be a funded account from your Ganache instance.
GANACHE_FUNDER_PRIVATE_KEY=

# The RPC URL for the blockchain.
# For Docker on Mac/Windows, this should be http://host.docker.internal:8545
# For running locally, this should be http://localhost:8545
BLOCKCHAIN_RPC_URL=http://host.docker.internal:8545

# --- AI Service Settings ---

# Your API key from Google Cloud Console for the Generative Language API
GEMINI_API_KEY=